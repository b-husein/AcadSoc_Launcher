{
  "manifest_version": 2,
  "name": "AcadSoc Launcher",
  "version": "1.0.0",

  "description": "AcadSoc tools launcher",
  "icons": {
    "90": "./img/icon_90.png",
    "128": "./img/icon_128.png",
    "150": "./img/icon_150.png"
  },

  "browser_action": {
    "default_icon": "./img/icon_150.png",
    "default_popup": "popup.html"
  },

  "author": "Husein B.",

  "permissions": ["activeTab"]
}
